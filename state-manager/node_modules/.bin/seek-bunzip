#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/andrey/projects/zknoid-backend/state-manager/node_modules/seek-bzip/bin/node_modules:/Users/andrey/projects/zknoid-backend/state-manager/node_modules/seek-bzip/node_modules:/Users/andrey/projects/zknoid-backend/state-manager/node_modules:/Users/andrey/projects/zknoid-backend/node_modules:/Users/andrey/projects/node_modules:/Users/andrey/node_modules:/Users/node_modules:/node_modules:/Users/andrey/projects/zknoid-backend/state-manager/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/andrey/projects/zknoid-backend/state-manager/node_modules/seek-bzip/bin/node_modules:/Users/andrey/projects/zknoid-backend/state-manager/node_modules/seek-bzip/node_modules:/Users/andrey/projects/zknoid-backend/state-manager/node_modules:/Users/andrey/projects/zknoid-backend/node_modules:/Users/andrey/projects/node_modules:/Users/andrey/node_modules:/Users/node_modules:/node_modules:/Users/andrey/projects/zknoid-backend/state-manager/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../seek-bzip/bin/seek-bunzip" "$@"
else
  exec node  "$basedir/../seek-bzip/bin/seek-bunzip" "$@"
fi
