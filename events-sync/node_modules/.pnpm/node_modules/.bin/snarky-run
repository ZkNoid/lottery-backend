#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/andrey/projects/zknoid-backend/events-sync/node_modules/.pnpm/o1js@1.3.1/node_modules/o1js/src/build/node_modules:/Users/andrey/projects/zknoid-backend/events-sync/node_modules/.pnpm/o1js@1.3.1/node_modules/o1js/src/node_modules:/Users/andrey/projects/zknoid-backend/events-sync/node_modules/.pnpm/o1js@1.3.1/node_modules/o1js/node_modules:/Users/andrey/projects/zknoid-backend/events-sync/node_modules/.pnpm/o1js@1.3.1/node_modules:/Users/andrey/projects/zknoid-backend/events-sync/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/andrey/projects/zknoid-backend/events-sync/node_modules/.pnpm/o1js@1.3.1/node_modules/o1js/src/build/node_modules:/Users/andrey/projects/zknoid-backend/events-sync/node_modules/.pnpm/o1js@1.3.1/node_modules/o1js/src/node_modules:/Users/andrey/projects/zknoid-backend/events-sync/node_modules/.pnpm/o1js@1.3.1/node_modules/o1js/node_modules:/Users/andrey/projects/zknoid-backend/events-sync/node_modules/.pnpm/o1js@1.3.1/node_modules:/Users/andrey/projects/zknoid-backend/events-sync/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../o1js/src/build/run.js" "$@"
else
  exec node  "$basedir/../o1js/src/build/run.js" "$@"
fi
