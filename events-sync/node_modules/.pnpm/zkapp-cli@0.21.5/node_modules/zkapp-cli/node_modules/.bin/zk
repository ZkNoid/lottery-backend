#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/andrey/projects/zknoid-backend/events-sync/node_modules/.pnpm/zkapp-cli@0.21.5/node_modules/zkapp-cli/src/bin/node_modules:/Users/andrey/projects/zknoid-backend/events-sync/node_modules/.pnpm/zkapp-cli@0.21.5/node_modules/zkapp-cli/src/node_modules:/Users/andrey/projects/zknoid-backend/events-sync/node_modules/.pnpm/zkapp-cli@0.21.5/node_modules/zkapp-cli/node_modules:/Users/andrey/projects/zknoid-backend/events-sync/node_modules/.pnpm/zkapp-cli@0.21.5/node_modules:/Users/andrey/projects/zknoid-backend/events-sync/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/andrey/projects/zknoid-backend/events-sync/node_modules/.pnpm/zkapp-cli@0.21.5/node_modules/zkapp-cli/src/bin/node_modules:/Users/andrey/projects/zknoid-backend/events-sync/node_modules/.pnpm/zkapp-cli@0.21.5/node_modules/zkapp-cli/src/node_modules:/Users/andrey/projects/zknoid-backend/events-sync/node_modules/.pnpm/zkapp-cli@0.21.5/node_modules/zkapp-cli/node_modules:/Users/andrey/projects/zknoid-backend/events-sync/node_modules/.pnpm/zkapp-cli@0.21.5/node_modules:/Users/andrey/projects/zknoid-backend/events-sync/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../src/bin/index.js" "$@"
else
  exec node  "$basedir/../../src/bin/index.js" "$@"
fi
