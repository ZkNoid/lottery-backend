#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/andrey/projects/zknoid-backend/data_updater/node_modules/uglify-js/bin/node_modules:/Users/andrey/projects/zknoid-backend/data_updater/node_modules/uglify-js/node_modules:/Users/andrey/projects/zknoid-backend/data_updater/node_modules:/Users/andrey/projects/zknoid-backend/node_modules:/Users/andrey/projects/node_modules:/Users/andrey/node_modules:/Users/node_modules:/node_modules:/Users/andrey/projects/zknoid-backend/data_updater/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/andrey/projects/zknoid-backend/data_updater/node_modules/uglify-js/bin/node_modules:/Users/andrey/projects/zknoid-backend/data_updater/node_modules/uglify-js/node_modules:/Users/andrey/projects/zknoid-backend/data_updater/node_modules:/Users/andrey/projects/zknoid-backend/node_modules:/Users/andrey/projects/node_modules:/Users/andrey/node_modules:/Users/node_modules:/node_modules:/Users/andrey/projects/zknoid-backend/data_updater/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../uglify-js/bin/uglifyjs" "$@"
else
  exec node  "$basedir/../uglify-js/bin/uglifyjs" "$@"
fi
